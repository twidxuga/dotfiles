Include ~/.colima/ssh_config

Host github-bunch
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519
  IdentitiesOnly yes

Host github-personal
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes

host *
  # reuse connections
  ControlMaster auto
  ControlPath ~/.ssh/ssh_mux_%n_%p_%r
  # disconnect if inactive after 15 minutes
  ControlPersist 900
