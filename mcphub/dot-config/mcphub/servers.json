{
  "mcpServers": {
    "fetch": {
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/fetch"
      ],
      "command": "docker",
      "disabled": false
    },
    "time": {
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/time"
      ],
      "command": "docker",
      "disabled": false
    },
    "tavily-remote": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.tavily.com/mcp/?tavilyApiKey=${TAVILY_API_KEY}"
      ],
      "disabled": false
    },
    "playwright": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "--init", "--pull=always", "mcr.microsoft.com/playwright/mcp"],
      "disabled": false
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest","--headless"],
      "disabled": false
    }
  },
  "nativeMCPServers": {
    "neovim": {
      "autoApprove": [],
      "custom_instructions": {
        "disabled": false
      },
      "disabled_tools": []
    }
  }
}
