{
  "nvim-snippets new snippet": {
    "description": "creates a new nvim-snippets snippet",
    "prefix": "snip",
    "body": [
      "\"${1:name}\": {",
      "  \"description\": \"${2:description}\",",
      "  \"prefix\": [\"${3:prefix}\"],",
      "  \"body\": [",
      "    \"$0\"",
      "  ]",
      "}"
    ]
  },
  "Key value pair": {
    "description": "key value pair",
    "prefix": ["kv"],
    "body": [
      "\"${1:key}\": \"${2:value}\"$0"
    ]
  }
}
